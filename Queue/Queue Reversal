
Input:
6
4 3 1 10 2 6

Output: 
6 2 10 1 3 4


//Function to reverse the queue.
queue<int> rev(queue<int> q)
{
   //using stack
   stack<int> s;
   //put q ele in stack
   while(!q.empty()){
       int element = q.front();
       q.pop();
       s.push(element);
   }
   //stack se ele nikl k put in queue
   while(!s.empty()){
       int element = s.top();
       s.pop();
       q.push(element);
   }
   return q;
}
